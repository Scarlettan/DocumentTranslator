<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="documenttranslator.controllers.TranslateCompleteController">
   <children>
      <Label fx:id="labelStatus" text="Translating..." />
      <ProgressBar fx:id="progressBar" progress="0.0" />
      <ButtonBar>
         <buttons>
            <Button fx:id="buttonOpenFile" mnemonicParsing="false" text="Open File..." />
            <Button fx:id="buttonOpenFolder" mnemonicParsing="false" text="Open Folder..." />
         </buttons>
      </ButtonBar>
      <HBox fx:id="hboxPadding" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
